<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE components PUBLIC "-//SEASAR//DTD S2Container 2.4//EN"
	"http://www.seasar.org/dtd/components24.dtd">
<!--
ActiveMQ を利用した JMS 送信のサンプル
-->
<components>

	<!-- Jms connectorの設定 -->
	<component class="org.mule.extras.seasar2.config.impl.JmsConnectorConfig" >
		<!-- connectionFactoryの設定 -->
		<property name="connectionFactoryConfig">
			<component class="org.mule.extras.seasar2.config.impl.ConnectionFactoryConfig" >
				<property name="factoryClassName">
					"org.apache.activemq.ActiveMQConnectionFactory"
				</property>
				<!-- -ActiveMQConnectionFactory のプロパティ-->
				<initMethod name="setProperty" >
					<arg>"brokerURL"</arg>
					<arg>"tcp://localhost:61616"</arg>
				</initMethod>
			</component>
		</property>
		
		<!-- その他のプロパティの設定 -->
		<initMethod name="setProperty" >
			<arg>"maxRedelivery"</arg>
			<arg>5</arg>
		</initMethod>
		
	</component>

	<!-- S2MuleSender の設定-->
	<component class="org.mule.extras.seasar2.sender.impl.S2MuleSenderImpl" >
		<property name="outboundUri">"jms://Foo"</property>
		<initMethod name="init"/>
	</component>

</components>
