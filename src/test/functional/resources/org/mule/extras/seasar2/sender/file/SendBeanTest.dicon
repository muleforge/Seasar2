<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE components PUBLIC "-//SEASAR//DTD S2Container 2.4//EN"
	"http://www.seasar.org/dtd/components24.dtd">
<components>
	
	<include path="s2mule-sender.dicon"/>

	<!-- File Connectorの設定-->
	<component class="org.mule.extras.seasar2.connector.impl.FileConnector">
        <property name="outputPattern">"S2Mule-Test-FILE-O-004-${DATE:yyyyMMdd}.txt"</property>
        <property name="serialiseObjects">true</property>		
	</component>

	<!-- S2MuleSender の設定-->
	<component class="org.mule.extras.seasar2.sender.impl.S2MuleSenderImpl" >
		<initMethod name="addOutboundEndpoint">
			<arg>"file:///C:/temp"</arg>
		</initMethod>
	</component>
	
</components>

