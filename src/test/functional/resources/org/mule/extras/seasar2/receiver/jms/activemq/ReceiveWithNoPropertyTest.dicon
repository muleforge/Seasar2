<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE components PUBLIC "-//SEASAR//DTD S2Container 2.4//EN"
	"http://www.seasar.org/dtd/components24.dtd">
<components>
    <include path="s2mule-receiver.dicon"/>
    <include path="j2ee.dicon"/>
    
	 <component name="echoUMO" 
                class="org.mule.extras.seasar2.receiver.common.EchoUMO" />
    
    <!-- ActiveMQ Jms connectorの設定 -->
    <component class="org.mule.extras.seasar2.config.impl.ActiveMQXAJmsConnectorConfigImpl" >
        <!-- brokerURLの設定 -->
        <property name="brokerURL">
                "tcp://localhost:61616"
        </property>
        <initMethod name="setProperty" >
            <arg>"specification"</arg>
            <arg>"1.1"</arg>
        </initMethod>
    </component>
    
    <component 
       name="jms_in" class="org.mule.extras.seasar2.config.impl.EndpointConfigImpl" >
     <property name="uri">"jms://Foo"</property>
    </component>
    
    <!-- MuleConfig -->
    <component class="org.mule.extras.seasar2.receiver.impl.S2MuleConfiguration" >
        <initMethod name="addInbounEndpoint">
            <arg>jms_in</arg>
        </initMethod>
        <property name="umoImpl">echoUMO</property>
    </component>
    
</components>
