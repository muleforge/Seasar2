<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE components PUBLIC "-//SEASAR//DTD S2Container 2.3//EN"
	"http://www.seasar.org/dtd/components23.dtd">
<components>
	<include path="s2mule-sender.dicon"/>
	
	<!-- Jms connectorの設定 -->
	<component class="org.mule.extras.seasar2.config.impl.ActiveMQXAJmsConnectorConfigImpl" >
		<!-- brokerURLの設定 -->
		<property name="brokerURL">
				"tcp://localhost:61616"
		</property>
		<!-- その他のプロパティの設定 -->
		<initMethod name="setProperty" >
			<arg>"maxRedelivery"</arg>
			<arg>5</arg>
		</initMethod>
	</component>
	
</components>
